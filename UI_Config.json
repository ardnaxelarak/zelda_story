{
    "output_file": "config.json",

    "liveOnly": {
        "type": "checkbox",
        "value": false,
        "label": "Live Only Mode",
        "tooltip": "Whether to only run while live",
        "group" : "General"
    },
    "command": {
        "type": "textbox",
        "value": "!story",
        "label": "Command",
        "tooltip": "Name of the Command",
        "group": "General"
    },
    "permission": {
        "type": "dropdown",
        "value": "Everyone",
        "items": ["Everyone","Regular","Subscriber","Moderator","Editor"],
        "label": "Permission level",
        "tooltip": "Permission level for the command",
        "group": "General"
    },
    "useCooldown": {
        "type": "checkbox",
        "value": true,
        "label": "Use Cooldown",
        "tooltip": "Enable/Disable cooldown and user cooldown",
        "group": "Cooldown"
    },
    "useCooldownMessages": {
        "type": "checkbox",
        "value": true,
        "label": "Use Cooldown Messages",
        "tooltip": "Enable/Disable cooldown messages",
        "group": "Cooldown"
    },
    "cooldown": {
        "type": "numberbox",
        "label": "Cooldown (seconds)",
        "value": 1,
        "tooltip": "Cooldown in seconds.",
        "group": "Cooldown"
    },
    "onCooldown": {
        "type": "textbox",
        "value": "$user, $command is still on cooldown for $cd seconds!",
        "label": "Cooldown Response",
        "tooltip": "The message that the bot will display when the command is on cooldown.\r\n$user = user\r\n$command = command name\r\n$cd = time remaining",
        "group": "Cooldown"
    },
    "userCooldown": {
        "type": "numberbox",
        "label": "User Cooldown (seconds)",
        "value": 300,
        "tooltip": "User cooldown in seconds.",
        "group": "Cooldown"
    },
    "onUserCooldown": {
        "type": "textbox",
        "value": "$user, $command is still on user cooldown for $cd seconds!",
        "label": "User Cooldown Response",
        "tooltip": "The message that the bot will display when the command is on user cooldown.\r\n$user = user\r\n$command = command name\r\n$cd = time remaining",
        "group": "Cooldown"
    }
}
